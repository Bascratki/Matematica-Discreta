A = {1, 2, 3, 4, 5}
num = 2

if num in A:
    print("a) ", num, "pertence a A")
else:
    print("a) ", num, "não pertence a A")
    
A = {1, 2, 3, 4, 5}
num = 6

if num in A:
    print("b) ", num, "pertence a A")
else:
    print("b) ", num, "não pertence a A")
    
A = {1, 2, 3, 4, 5}
num = set()

if num in A:
    print("c) ", num, "pertence a A")
else:
    print("c) ", num, "não pertence a A")